@model UpdateProjectDto

@{
    ViewData["Title"] = "UpdateProject";
    Layout = "~/Areas/Admin/Views/AdminUILayout/Index.cshtml";
}

<div class="card">
    <div class="card-body">

        <h4 class="card-title">Update Project</h4>
        <p class="card-subtitle mb-4">**Fill The All Blanks**</p>

        <form method="post">
            <input type="hidden" asp-for="ProjectId" />
            <div class="form-group">
                <label for="exampleInputSideName">Project Name</label>
                <input class="form-control" id="exampleInputSideName" asp-for="Name" placeholder="Enter Project Name">
            </div>
            <div class="form-group">
                <label for="exampleInputJobTitle">Job Title</label>
                <input class="form-control" id="exampleInputJobTitle" asp-for="JobTitle" placeholder="Job Title">
            </div>
            <div class="form-group">
                <label for="exampleInputCoverImageUrl">Cover Image Url</label>
                <input class="form-control" id="exampleInputCoverImageUrl" asp-for="CoverImageUrl" placeholder="Cover Image Url">
            </div>


            <button type="submit" class="btn btn-primary waves-effect waves-light">Update Project</button>
        </form>

    </div> <!-- end card-body-->
</div>

